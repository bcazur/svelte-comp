{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "module": "dist/index.mjs",
  "main": "dist/index.umd.js",
  "types": "dist/ts/index.d.ts",
  "svelte": "dist/libindex.js",
  "scripts": {
    "build": "rollup -c",
    "build-lib": "rollup -c rollup.lib.config.js",
